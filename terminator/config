  suppress_multiple_term_dialog = True
  title_transmit_bg_color = "#38944f"
[keybindings]
  close_term = <Primary><Shift>w
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
